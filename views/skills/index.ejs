<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>Developer Skills</title>
</head>
<body>
  <a href="/">Home</a>
  <h1>All Developers</h1>
  <ul>
      <% developers.forEach(function(dev) { %>
        <li>
          <a href="/skills/<%= dev.id %>"><%= dev.name %></a>
        </li>
      <% }); %>
  </ul>
  </body>
</html>